{
    "name": "stackerjs-db-mysql-adapter",
    "version": "1.1.1",
    "author": {
        "name": "Vinicius Guedes",
        "email": "viniciusgued@gmail.com",
        "url": "https://vinyguedess.github.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/parpeoficial/stackerjs-db-mysql-adapter"
    },
    "main": "lib/index.js",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "mysql": "^2.15.0",
        "stackerjs-types": "^1.1.2",
        "stackerjs-utils": "^1.1.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "lint-staged": "^7.0.4",
        "mocha": "^5.1.1",
        "nyc": "^11.7.1"
    },
    "lint-staged": {
        "*.js": [
            "npm run lint",
            "git add ."
        ]
    },
    "nyc": {
        "exclude": [
            "lib",
            "test",
            "index.js"
        ],
        "require": [
            "babel-register",
            "babel-polyfill"
        ],
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "scripts": {
        "precommit": "./node_modules/.bin/lint-staged",
        "lint": "eslint src/ test/ --fix",
        "build": "./node_modules/.bin/babel src -d lib",
        "test": "NODE_ENV=dev ./node_modules/.bin/nyc ./node_modules/mocha/bin/_mocha"
    }
}
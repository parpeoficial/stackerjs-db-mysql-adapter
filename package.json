{
  "name": "stackerjs-db-mysql-adapter",
  "version": "0.0.0-development",
  "author": {
    "name": "Vinicius Guedes",
    "email": "viniciusgued@gmail.com",
    "url": "https://vinyguedess.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parpeoficial/stackerjs-db-mysql-adapter"
  },
  "main": "lib/index.js",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "mysql": "^2.15.0",
    "stackerjs-types": "^1.1.2",
    "stackerjs-utils": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "semantic-release": "^15.5.1",
    "travis-deploy-once": "^5.0.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add ."
    ]
  },
  "nyc": {
    "exclude": [
      "lib",
      "test",
      "index.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "scripts": {
    "precommit": "./node_modules/.bin/lint-staged",
    "lint": "eslint src/ test/ --fix",
    "build": "./node_modules/.bin/babel src -d lib",
    "test": "NODE_ENV=dev ./node_modules/.bin/nyc ./node_modules/mocha/bin/mocha",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  }
}
